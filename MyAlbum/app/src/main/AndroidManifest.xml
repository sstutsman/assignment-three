<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.imac.myalbum" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".myAlbum"
            android:label="@string/title_activity_my_album" >
        </activity>
        <activity
            android:name=".myAlbumAdapter"
            android:label="@string/title_activity_my_album_adapter" >
        </activity>
        <activity
            android:name=".myAlbumView"
            android:label="@string/title_activity_my_album_view" >
        </activity>
    </application>

</manifest>
