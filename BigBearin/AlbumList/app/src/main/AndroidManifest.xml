<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.albumlist.albumlist" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".SecondaryActivity"
            android:label="@string/app_name2">
        </activity>

        <activity
            android:name=".EscapeActivity"
            android:label="@string/escape_string">
        </activity>

        <activity
            android:name=".BornActivity"
            android:label="@string/born_string">
        </activity>

        <activity
            android:name=".DarkActivity"
            android:label="@string/dark_string">
        </activity>

    </application>

</manifest>
